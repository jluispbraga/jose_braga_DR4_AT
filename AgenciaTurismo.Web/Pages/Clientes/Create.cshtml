@page
@model AgenciaTurismo.Web.Pages.Clientes.CreateModel

<h2>Novo Cliente</h2>

<form method="post">
    <label>Nome:</label>
    <input asp-for="Cliente.Nome" />
    <span asp-validation-for="Cliente.Nome"></span>

    <label>Email:</label>
    <input asp-for="Cliente.Email" />
    <span asp-validation-for="Cliente.Email"></span>

    <button type="submit">Salvar</button>
</form>

<a asp-page="./Index">Voltar</a>

@section Scripts { <partial name="_ValidationScriptsPartial"/> }