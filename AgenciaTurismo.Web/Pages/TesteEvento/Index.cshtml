@page
@model AgenciaTurismo.Web.Pages.TesteEvento.IndexModel
@{
    ViewData["Title"] = "Teste Evento CapacityReached";
}

<h2>Teste Evento CapacityReached</h2>

@if (!string.IsNullOrEmpty(Model.MensagemEvento))
{
    <p style="color:red">@Model.MensagemEvento</p>
}
else
{
    <p>Nenhum evento disparado ainda.</p>
}